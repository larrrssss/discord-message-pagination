{
  "name": "discord-msg-pagination",
  "version": "1.0.0",
  "description": "A pagination utility for Messages in discord.js using message components",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src/ --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "test:bot": "ts-node --files test/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/larrrssss/discord-message-pagination.git"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  },
  "keywords": [
    "npm package",
    "template"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/larrrssss/discord-message-pagination/issues"
  },
  "homepage": "https://github.com/larrrssss/discord-message-pagination#readme",
  "dependencies": {
    "discord.js": "^13.1.0"
  }
}
